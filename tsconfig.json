        "allowJs": true,
        "strict": true,
        "noEmit": true,
        "module": "ES6",
        "lib": ["ES2020"],
        "moduleResolution": "node16",
        "resolveJsonModule": true,
        "isolatedModules": false,        
        "esModuleInterop": true
